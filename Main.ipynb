    "import imaplib\n",
    "import email\n",
    "import os\n",
    "import mimetypes\n",
    "import pandas as pd\n",
    "import hashlib\n",
    "from io import BytesIO\n",
    "import PyPDF2\n",
    "import time\n",
    "\n",
    "# Connect to the email server\n",
    "def connect_to_email():\n",
    "    email_user = 'youremail@example.com'  # Replace with your email\n",
    "    email_pass = 'yourpassword'     # Replace with your app password or regular password\n",
    "\n",
    "    try:\n",
    "        print(\"Connecting to IMAP server...\")\n",
    "        mail = imaplib.IMAP4_SSL('imap.gmail.com', timeout=60)  # Adjust as necessary\n",
    "        print(\"Connected to server, logging in...\")\n",
    "        mail.login(email_user, email_pass)\n",
    "        print(\"Logged in successfully.\")\n",
    "        mail.select('inbox')  # Change this to the desired folder\n",
    "        return mail\n",
    "    except imaplib.IMAP4.error as e:\n",
    "        print(f\"IMAP error: {e}\")\n",
    "    except Exception as e:\n",
    "        print(f\"Error connecting to email: {e}\")\n",
    "        return None\n",
    "\n",
    "def extract_attachments(msg):\n",
    "    attachments = []\n",
    "    for part in msg.walk():\n",
    "        if part.get_content_disposition() == 'attachment':\n",
    "            filename = part.get_filename()\n",
    "            if filename:\n",
    "                file_data = part.get_payload(decode=True)\n",
    "                file_type = part.get_content_type()\n",
    "                attachments.append((filename, file_data, file_type))\n",
    "    return attachments\n",
    "\n",
    "def fetch_emails(mail, limit=100, retry_count=3):\n",
    "    attachments = []\n",
    "    \n",
    "    try:\n",
    "        mail.select('inbox')\n",
    "    except Exception as e:\n",
    "        print(f\"Error selecting inbox: {e}\")\n",
    "        return attachments\n",
    "\n",
    "    status, data = mail.search(None, 'ALL')  \n",
    "    if status != 'OK':\n",
    "        print(\"No messages found!\")\n",
    "        return attachments\n",
    "\n",
    "    email_ids = data[0].split()\n",
    "    print(f\"Found {len(email_ids)} emails.\")\n",
    "\n",
    "    if limit is not None:\n",
    "        email_ids = email_ids[:limit]\n",
    "\n",
    "    for e_id in email_ids:\n",
    "        try:\n",
    "            result, message_data = mail.fetch(e_id, '(RFC822)')\n",
    "            if result != 'OK':\n",
    "                print(f\"Error fetching email ID {e_id}: {result}\")\n",
    "                continue\n",
    "\n",
    "            raw_email = message_data[0][1]\n",
    "            msg = email.message_from_bytes(raw_email)\n",
    "\n",
    "            sender_email = msg['From']\n",
    "            print(f\"Processing email from: {sender_email}\")\n",
    "\n",
    "            extracted_attachments = extract_attachments(msg)\n",
    "\n",
    "            for filename, file_data, file_type in extracted_attachments:\n",
    "                try:\n",
    "                    print(f\"Processing attachment: {filename}\")\n",
    "                    is_password_protected = check_password_protection(filename, file_data) if file_type == 'application/pdf' else False\n",
    "                    is_malicious = scan_file_for_malicious(filename, file_data)\n",
    "\n",
    "                    attachments.append({\n",
    "                        'sender_email': sender_email,\n",
    "                        'filename': filename,\n",
    "                        'file_type': file_type,\n",
    "                        'is_password_protected': is_password_protected,\n",
    "                        'is_malicious': is_malicious\n",
    "                    })\n",
    "\n",
    "                except Exception as e:\n",
    "                    print(f\"Error processing attachment {filename}: {e}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"Error processing email ID {e_id}: {e}\")\n",
    "            if retry_count > 0 and 'socket error' in str(e).lower():\n",
    "                print(\"Reconnecting to the mail server...\")\n",
    "                time.sleep(5)  # Wait for a few seconds before retrying\n",
    "                mail = connect_to_email()  # Reinitialize the mail connection\n",
    "                if mail:\n",
    "                    return fetch_emails(mail, limit, retry_count - 1)\n",
    "                else:\n",
    "                    print(\"Failed to reconnect to the mail server.\")\n",
    "                    return attachments  # Return what has been processed so far\n",
    "\n",
    "    return attachments\n",
    "\n",
    "def check_password_protection(filename, file_data):\n",
    "    try:\n",
    "        pdf_reader = PyPDF2.PdfReader(BytesIO(file_data))\n",
    "        return pdf_reader.is_encrypted\n",
    "    except Exception as e:\n",
    "        print(f\"Error checking password protection for {filename}: {e}\")\n",
    "        return False\n",
    "\n",
    "def scan_file_for_malicious(filename, file_data):\n",
    "    file_hash = hashlib.sha256(file_data).hexdigest()\n",
    "    known_malware_hashes = load_known_malware_hashes()\n",
    "    \n",
    "    if file_hash in known_malware_hashes:\n",
    "        return True  # File is malicious\n",
    "\n",
    "    if filename.endswith(('.exe', '.bat', '.vbs', '.js', '.scr')):\n",
    "        return True  # Suspicious file types often used for malware\n",
    "\n",
    "    if filename.endswith('.pdf') and contains_obfuscated_content(file_data):\n",
    "        return True  # Suspicious PDFs with obfuscated content\n",
    "\n",
    "    return False\n",
    "\n",
    "def load_known_malware_hashes():\n",
    "    return [\n",
    "        'd41d8cd98f00b204e9800998ecf8427e', \n",
    "        'e99a18c428cb38d5f260853678922e03',\n",
    "    ]\n",
    "\n",
    "def contains_obfuscated_content(file_data):\n",
    "    try:\n",
    "        pdf_reader = PyPDF2.PdfReader(BytesIO(file_data))\n",
    "        for page in pdf_reader.pages:\n",
    "            text = page.extract_text()\n",
    "            if \"JavaScript\" in text or \"eval\" in text:\n",
    "                return True\n",
    "    except Exception as e:\n",
    "        print(f\"Error analyzing PDF content: {e}\")\n",
    "    return False\n",
    "\n",
    "def generate_csv_report(attachments):\n",
    "    df = pd.DataFrame(attachments)\n",
    "    df.to_csv('email_attachments_report.csv', index=False)\n",
    "    print(\"CSV report generated: email_attachments_report.csv\")\n",
    "\n",
    "def main():\n",
    "    mail = connect_to_email()\n",
    "    if mail:\n",
    "        attachments = fetch_emails(mail)\n",
    "        if attachments:\n",
    "            generate_csv_report(attachments)\n",
    "        else:\n",
    "            print(\"No attachments processed.\")\n",
    "        mail.logout()\n",
    "    else:\n",
    "        print(\"Failed to connect to email.\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
